<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="15.0dip" />
            <gradient android:startColor="#ffdddddd" android:endColor="#ffdddddd" android:angle="270.0" android:centerY="0.5" android:centerColor="#ffffffff" />
            <stroke android:width="1.0dip" android:color="#ff689F38" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="15.0dip" />
                <gradient android:startColor="#ff8BC34A" android:endColor="#ff689F38" android:angle="90.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="15.0dip" />
                <gradient android:startColor="#ff8BC34A" android:endColor="#ff689F38" android:angle="90.0" />
            </shape>
        </clip>
    </item>
</layer-list>